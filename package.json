{
  "name": "urban-assault-level-maker",
  "version": "1.0.0",
  "description": "Level maker for game Urban Assault",
  "scripts": {
    "build": "tsc",
    "compile:css": "node-sass src/sass/main.scss app.comp.css",
    "compress:css": "node-sass app.comp.css app.css --output-style compressed",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "run:electron": "electron ./dist/main.js",
    "start": "npm-run-all build compile:css compress:css run:electron"
  },
  "keywords": [
    "Electron",
    "typescript"
  ],
  "author": "Dubstabber",
  "license": "CC0-1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "electron": "^13.1.7",
    "eslint": "^7.32.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {}
}
